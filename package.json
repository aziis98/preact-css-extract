{
    "name": "preact-css-extract",
    "version": "0.0.15",
    "description": "Compile-time CSS extraction and Preact classList helper for clean component styling",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        },
        "./plugin": {
            "import": "./dist/plugin.js"
        },
        "./comptime": {
            "import": "./dist/comptime/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "preact-classlist.d.ts",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build": "rollup -c",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "preact",
        "css",
        "vite",
        "styling",
        "css-in-js",
        "compile-time"
    ],
    "author": "aziis98",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/aziis98/preact-css-extract.git"
    },
    "bugs": {
        "url": "https://github.com/aziis98/preact-css-extract/issues"
    },
    "homepage": "https://github.com/aziis98/preact-css-extract#readme",
    "devDependencies": {
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/bun": "latest",
        "rollup": "^4.52.5",
        "rollup-plugin-dts": "^6.2.3",
        "tslib": "^2.8.1",
        "typescript": "^5"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "clsx": "^2.1.1",
        "preact": "^10.27.2",
        "vite": "^7.2.0"
    }
}
